    "class AttentionLayer(torch.nn.Module):\n",
    "    def __init__(self, d_model):\n",
    "        super(AttentionLayer, self).__init__()\n",
    "\n",
    "        self.d_model = d_model\n",
    "\n",
    "        self.W_query = torch.nn.Linear(d_model, d_model, bias=False)\n",
    "        self.W_key = torch.nn.Linear(d_model, d_model, bias=False)\n",
    "        self.W_value = torch.nn.Linear(d_model, d_model, bias=False)\n",
    "        self.W_output = torch.nn.Linear(d_model, d_model, bias=False)\n",
    "\n",
    "        self.softmax = torch.nn.Softmax(dim=1)\n",
    "\n",
    "    def scaled_dot_product(self, Q, K, V):\n",
    "        numerator = torch.matmul(Q, K.transpose(-2, -1)) # QK^t\n",
    "        denominator = math.sqrt(self.d_model) # (d)^1/2\n",
    "        smax = self.softmax(numerator / denominator) # softmax(QK^t/(d^1/2))\n",
    "        output = torch.matmul(smax, V) # softmax * V\n",
    "        return output\n",
    "        \n",
    "    def forward(self, Q, K, V):\n",
    "        Q = self.W_query(Q)\n",
    "        K = self.W_key(K)\n",
    "        V = self.W_value(V)\n",
    "\n",
    "        attention = self.scaled_dot_product(Q, K, V)\n",
    "\n",
    "        output = self.W_output(attention)\n",
    "        return output"
   ]
